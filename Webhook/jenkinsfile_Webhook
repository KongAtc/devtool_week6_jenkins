pipeline {
   agent any
    stages {
         stage('pre -build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }
        stage('build') {
            steps {
                
                echo 'build in progress'
                sh 'python3  Webhook/main.py'
            }
        }
        stage('unit-test') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }
        stage('deploy') {
            steps {
                
                echo 'build in progress'
                sh 'python3  Webhook/main.py'
            }
        }
    }
}
